{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
      "allowJs": true,
      "incremental": true,
      "jsx": "preserve",
      "noEmit": true,
      "module": "ESNext",
      "plugins": [
        {
          "name": "next"
        }
      ],
      "skipLibCheck": true
  },
  "include": [
      "src",
      ".next/types/**/*.ts"
  ],
  "exclude": [
      ".next",
      "node_modules"
  ]
}
